{
  "functions": {
    "api/websocket": {
      "handler": "api/websocket.js",
      "memory": 1024,
      "events": [
        {
          "http": {
            "method": "get",
            "path": "/websocket"
          }
        },
        {
          "websocket": {
            "route": "/",
            "allowCors": true
          }
        }
      ]
    }
  }
}
